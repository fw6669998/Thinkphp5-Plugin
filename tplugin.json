{

  //数据库字段提示方法(方法名.参数位置), 参数可不设置, 默认为第一个参数, test("字段提示")
  "dbMethod": [
    "test",
    //示例
    "save.2"
  ],
  //数据库字段提示方法, 数据参数, test(["字段提示"=>100])
  "dbArrMethod": [
    "test"
  ],
  //数据库提示字段, res["数据库字段"]=100
  "dbVar": [
    "res",
    "map",
    "where",
    "data",
    "this->data"
  ],

  //日志开关,不设置默认关闭,该设置重启软件生效
  "logEnable": false,
  //通过前缀筛选日志,//最好从日志文件中复制前缀,如[ sql ] [ SQL ]
  "logPrefix": [
    "[ info ] [ PARAM ]"
  ],
  //通过正则筛选日志
  "logRegex":[
    "^.*\\[\\sSQL ((?!COLUMNS).)*$"  //显示带有[ sql但没是COLUMNS的记录
  ]
}